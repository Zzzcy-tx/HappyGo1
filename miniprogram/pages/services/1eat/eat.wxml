<view class="allMid">
  <view class="productContainer">

    <view class="product" wx:for="{{goodsList}}" wx:key="id" bindtap="goToDetail" data-id="{{item.id}}">
      <image class="productImage" src="{{item.pic}}" mode="aspectFill"/>
      <view class="shopInfoBox">
        <text class="shopName">{{item.name}}    {{item.address}}</text>
      </view>
    </view>

    <canvas style="width: 200px; height: 200px;margin:0 auto" canvas-id="myQrcode"></canvas>
    
  </view>
</view>
